---
const JsGeniusCorner =
  "data:image/png;base64,";

const JsJive =
  "data:image/png;base64,";

const CrackingTheCode =
  "data:image/png;base64,";

const EvergreenScript =
  "data:image/png;base64,";

const FreshOffTheCodebas =
  "data:image/png;base64,";

type Props = {
  title: string;
  subtitle: string;
};
const { title, subtitle } = Astro.props;
let icon = null;

switch (title) {
  case "JS Jive":
    icon = JsJive;
    break;
  case "Cracking the Code":
    icon = CrackingTheCode;
    break;
  case "Evergreen Scripts":
    icon = EvergreenScript;
    break;
  case "Fresh off the Codebase":
    icon = FreshOffTheCodebas;
    break;
  case "JS Genius Corner":
    icon = JsGeniusCorner;
    break;
}
---

<table style={{ width: "100%" }}>
    <tr>
        <td style={{ width: "100px", height: "80px", paddingRight: "20px" }}>
            <img style={{ backgroundColor: '#fff', width: "100px", height: "90px" }} src={icon} alt="Your Icon" />
        </td>
        <td>
            <h1 style={{ fontSize: "20px", fontFamily: '"ATT Aleck Sans Bold"', marginBottom: "0" }}>{title}</h1>
            <h2 style={{ fontSize: "18px", fontWeight: "400", textAlign: "left", fontFamily: '"ATT Aleck Sans Light"', marginTop: "0" }}>{subtitle}</h2>
        </td>
    </tr>
    <tr>
        <td colspan="2" style={{ paddingTop: "30px", paddingBottom: "50px" }}>
            <slot />
        </td>
    </tr>
</table>